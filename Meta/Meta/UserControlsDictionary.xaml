<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:Meta.Views"
                    xmlns:setupv="clr-namespace:Meta.Views.Setup"
                    xmlns:usercontrolsv="clr-namespace:Meta.Views.UserControls">

    <DataTemplate DataType="{x:Type v:HomeViewModel}">
        <v:HomeView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type setupv:AccountViewModel}">
        <setupv:AccountView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type setupv:DatabaseViewModel}">
        <setupv:DatabaseView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type setupv:LedgerViewModel}">
        <setupv:LedgerView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type setupv:AdminViewModel}">
        <setupv:AdminView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type usercontrolsv:AddOrganizerViewModel}">
        <usercontrolsv:AddOrganizerView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type usercontrolsv:ViewObjectsViewModel}">
        <usercontrolsv:ViewObjectsView />
    </DataTemplate>

</ResourceDictionary>